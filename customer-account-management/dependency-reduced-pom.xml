  <modelVersion>4.0.0</modelVersion>
  <artifactId>apispeciesms</artifactId>
  <name>apispeciesms</name>
  <version>0.0.1-SNAPSHOT</version>
  <description>Demo project for Spring Boot</description>
  <developers>
    <developer />
  </developers>
  <licenses>
    <license />
  </licenses>
  <scm />
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.5.1</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <mainClass>episen.CustomerAccount.CustomerAccountManagementApplication</mainClass>
          <layout>JAR</layout>
        </configuration>
      </plugin>
    </plugins>
  </build>
